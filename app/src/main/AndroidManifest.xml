<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.daniel.ersteapp">



    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".GetHighscoreActivity"
            android:label="@string/title_activity_get_highscore"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ShowHighScoreActivity"
            android:label="@string/title_activity_show_high_score"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ZehnActivity"
            android:label="ZehnActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".HundertActivity"
            android:label="HundertActivity"
            android:theme="@style/AppTheme" />
    </application>

</manifest>