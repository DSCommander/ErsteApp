<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.daniel.ersteapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GetHighscoreActivity"
            android:label="Eingabe Highscore"
            android:theme="@style/AppTheme"
            android:title="Eingabe Highscore"/>
        <activity
            android:name=".ShowHighScoreActivity"
            android:label="Highscore Zehn"
            android:theme="@style/AppTheme"
            android:title="Highscore Zehn"/>
        <activity
            android:name=".ZehnActivity"
            android:label="Spiel Zehn"
            android:theme="@style/AppTheme"
            android:title="Spiel Zehn"/>
        <activity
            android:name=".HundertActivity"
            android:label="Spiel Hundert"
            android:theme="@style/AppTheme"
            android:title="Spiel Hundert"/>
        <activity
            android:name=".HundertShowHighscoreActivity"
            android:label="Highscore Hundert"
            android:theme="@style/AppTheme"
            android:title="Highscore Hundert"/>
        <activity
            android:name=".HundertGetHighscoreActivity"
            android:label="Eingabe Highscore"
            android:theme="@style/AppTheme"
            android:title="Eingabe Highscore">
        </activity>
    </application>

</manifest>